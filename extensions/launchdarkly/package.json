{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "launchdarkly",
  "title": "LaunchDarkly",
  "description": "Control LaunchDarkly from Raycast",
  "icon": "command-icon.png",
  "author": "GLaDO8",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "show-feature-flags",
      "title": "Show Feature Flags",
      "description": "",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "access-token",
      "description": "Your LaunchDarkly access token",
      "type": "textfield",
      "required": true,
      "title": "Enter your LaunchDarkly access token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1",
    "@raycast/utils": "^1.10.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
